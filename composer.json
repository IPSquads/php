{
    "name": "ipsquads/php",
    "description": "IPSquads official PHP library for IP Address Geolocation, Timezone, Currency, Network & other information.",
    "keywords": [
        "ipsquads",
        "php"
    ],
    "homepage": "https://github.com/ipsquads/php",
    "license": "MIT",
    "authors": [
        {
            "name": "Sdklab007",
            "email": "support@ipsquads.com",
            "homepage": "https://ipsquads.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/cache": "4.4.15"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^7.5.16",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Ipsquads\\Php\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ipsquads\\Php\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
